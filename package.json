{
  "name": "mopidy-anjuna",
  "version": "0.0.1",
  "description": "Mopidy Web Frontend",
  "repository": {
    "type": "git",
    "url": "git@github.com:Hanse/mopidy-anjuna"
  },
  "main": "index.js",
  "scripts": {
    "watch-js": "watchify src/index.js -d -o public/app.js -v",
    "watch-css": "stylus --use nib -w assets/stylesheets/style.styl -o public/app.css",
    "watch": "npm run watch-js & npm run watch-css & npm run start",
    "start": "http-server",
    "test": "istanbul cover _mocha -- src/**/__tests__/*-test.js --compilers js:babel/register && istanbul report cobertura",
    "lint": "jsxhint src/ && jscs src/ --esnext; true",
    "babel-node": "babel-node --experimental --modules=common"
  },
  "author": "Hanse",
  "license": "MIT",
  "keywords": [
    "skeleton",
    "setup",
    "browserify",
    "react",
    "stylus",
    "nib",
    "make",
    "gnu make"
  ],
  "dependencies": {
    "album-cover": "0.0.3",
    "browserify": "^9.0.7",
    "flux": "^2.0.1",
    "mopidy": "^0.5.0",
    "object-assign": "^2.0.0",
    "react": "^0.13.1",
    "react-router": "^0.13.2"
  },
  "devDependencies": {
    "babel": "^5.0.12",
    "babelify": "^6.0.2",
    "http-server": "^0.8.0",
    "nib": "^1.1.0",
    "stylus": "^0.50.0",
    "uglify-js": "^2.4.19",
    "watchify": "latest"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "modules": "common"
        }
      ]
    ]
  }
}
